	author = {Kulkarni, Ketav and Habila, Nathan and Del Borgo, Mark P. and Aguilar, Marie-Isabel},
	month = feb,
	year = {2019},
	keywords = {Beta peptide, Biomaterials, Peptide materials, Supramolecular selfassembly, β-Peptide, β-amino acid containing peptides},
	pages = {70},
}

