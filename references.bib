@article{evans_ultra-violet_1974,
	title = {Ultra-violet and {X}-ray photoelectron spectroscopy studies of oxygen chemisorption on copper, silver and gold},
	volume = {58},
	issn = {0301-7249},
	url = {http://pubs.rsc.org/en/content/articlelanding/1974/dc/dc9745800097},
	doi = {10.1039/DC9745800097},
	abstract = {A photoelectron study (HeI, HeII and Mg Kα) of the oxidation at ordinary temperatures of polycrystalline copper, silver and gold, by both ground-state molecular and excited oxygen has established which oxidation states of these metals are formed during and after the dissociative chemisorption of oxygen. Up to about a monolayer coverage, copper is converted to the Cu(I) state and thereafter, as oxygen uptake proceeds, to the Cu(II) state. Silver also initially forms a + 1 oxidation state, but both the magnitude of the line widths of, and the significant absence of satellites associated with the core-level peaks suggest that a diamagnetic Ag(III) oxide rather than Ag(II) is formed upon further oxidation. Gold does not proceed beyond the Au(I) state. The densities of states (as reflected in the HeII spectra) of the three metals offer less guidance as to the conditions or ease of formation of the M(I) oxide overlayers, than the energy differences between the s-band edges and the vacuum level.},
	language = {en},
	number = {0},
	urldate = {2018-12-19},
	journal = {Faraday Discussions of the Chemical Society},
	author = {Evans, S. and Evans, E. L. and Parry, D. E. and Tricker, M. J. and Walters, M. J. and Thomas, J. M.},
	month = jan,
	year = {1974},
	pages = {97--105},
}

@article{duke_active_2015,
	title = {Active {Sites} in {Copper}-{Based} {Metal}–{Organic} {Frameworks}: {Understanding} {Substrate} {Dynamics}, {Redox} {Processes}, and {Valence}-{Band} {Structure}},
	volume = {119},
	issn = {1932-7447, 1932-7455},
	shorttitle = {Active {Sites} in {Copper}-{Based} {Metal}–{Organic} {Frameworks}},
	url = {http://pubs.acs.org/doi/10.1021/acs.jpcc.5b08053},
	doi = {10.1021/acs.jpcc.5b08053},
	abstract = {We have developed an integrated approach that combines synthesis, X-ray photoelectron spectroscopy (XPS) studies, and theoretical calculations for the investigation of active unsaturated metal sites (UMS) in copper-based metal−organic frameworks (MOFs). Speciﬁcally, extensive reduction of Cu+2 to Cu+1 at the MOF metal nodes was achieved. Introduction of mixed valence copper sites resulted in signiﬁcant changes in the valence band structure and an increased density of states near the Fermi edge, thereby altering the electronic properties of the copper-based framework. The development of mixedvalence MOFs also allowed tuning of selective adsorbate binding as a function of the UMS oxidation state. The presented studies could signiﬁcantly impact the use of MOFs for heterogeneous catalysis and gas puriﬁcation as well as foreshadow a new avenue for controlling the conductivity of typically insulating MOF materials.},
	language = {en},
	number = {49},
	urldate = {2018-12-18},
	journal = {The Journal of Physical Chemistry C},
	author = {Duke, Audrey S. and Dolgopolova, Ekaterina A. and Galhenage, Randima P. and Ammal, Salai C. and Heyden, Andreas and Smith, Mark D. and Chen, Donna A. and Shustova, Natalia B.},
	month = dec,
	year = {2015},
	keywords = {Cu, XPS},
	pages = {27457--27466},
}

